{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "activeTab",
    "http://localhost/*",
    "http://bytespaces/*"
  ],

  "web_accessible_resources": [
    "assets/*"
  ]
}